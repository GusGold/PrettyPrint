{
	"name": "PrettyPrint",
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png", 
		"128": "icon_128.png"
	},
	"version": "0.0.22",
	"description": "Javascript and CSS formatter/syntax highlighter",
	"content_scripts": [{
		"matches": [ "http://*/*", "https://*/*", "ftp://*/*" ],
		"js": ["bootstrap.js"],
		"run_at" : "document_end"
	}, {
		"matches": [ "file:///*" ],
		"js": [ "WebInspector.js", "beautify.js", "cssParser.js", "bootstrap.js", "content.js"],
		"run_at" : "document_end"
	}],
	"background_page" : "background.html",
	"options_page": "options.html",
	"permissions": [ "contextMenus", "tabs", "http://*/",  "https://*/", "ftp://*/" ]
}